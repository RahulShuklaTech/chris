{
  "expo": {
    "name": "StudyBot",
    "slug": "study-bot",
    "scheme": "study-bot",
    "description": "An academic coaching app for college students",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#025476"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "io.studybot.studybot",
      "supportsTablet": false,
      "buildNumber": "1.0.0",
      "infoPlist": {},
      "usesIcloudStorage": false
    },
    "android": {
      "versionCode": 1,
      "package": "io.studybot.studybot",
      "permissions": [
        "VIBRATE"
      ]
    },
    "web": {
      "themeColor": "#000000"
    },
    "notification": {
      "androidMode": "default"
    }
  }
}